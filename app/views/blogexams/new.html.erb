<%= form_with(model: @blogexam, local: true) do |form| %>
  <div class="blogexam_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="blogexam_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>